if [ -d ${XDG_CONFIG_HOME:-$HOME/.config}/eddos-shell/aliases ]; then for f in $(find "${XDG_CONFIG_HOME:-$HOME/.config}/eddos-shell/aliases/" -type f); do source $f; done; fi
if [ -d ${XDG_CONFIG_HOME:-$HOME/.config}/eddos-shell/environment ]; then for f in $(find "${XDG_CONFIG_HOME:-$HOME/.config}/eddos-shell/environment/" -type f); do source $f; done; fi
if [ -d ${XDG_CONFIG_HOME:-$HOME/.config}/eddos-shell/path ]; then for f in $(find "${XDG_CONFIG_HOME:-$HOME/.config}/eddos-shell/path/" -type f); do source $f; done; fi
